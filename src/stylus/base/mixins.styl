clear()
  clear             both

clearfix()
  *zoom:1

  &:before,
  &:after
    content         ""
    display         table

  &:after
    clear         both
    
float-comp()
  float    left
  display    inline 
  min-height  1px
    
unstyled-list()
  list-style          none
  margin            0
  padding           0
  
inline-list()
  li
    display         inline-block  